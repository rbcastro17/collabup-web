{
    "name": "CollabUp",
    "description": "Collaboration At its Finest",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "spatie/laravel-google-calendar": "^1.0",
        "anhskohbo/no-captcha": "^2.2",
        "matriphe/imageupload": "^5.2",
        "intervention/image": "^2.3",
        "a6digital/laravel-default-profile-image": "^1.0",
        "pusher/pusher-php-server": "^2.6",
        "netshell/paypal": "dev-master",
        "maddhatter/laravel-fullcalendar": "^1.2",
        "edofre/laravel-fullcalendar-scheduler": "*",
        "fxp/composer-asset-plugin": "^1.3",
        "cmgmyr/messenger": "^2.14",
        "brozot/laravel-fcm": "^1.2",
        "laravolt/avatar": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
"repositories": [
{
"type": "composer",
"url": "https://packagist.org"
},
{ "packagist": false }
]
}
